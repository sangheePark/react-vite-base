{
	"/users": {
		"_config": true,
		"description": "쿼리스트링을 통한 검색 미들웨어",
		"middlewares": ["_AdvancedSearch"]
	},
	"/image": {
		"_config": true,
		"description": "이미지 파일",
		"fetch": ".mock/data/mockserverlogo.png",
		"fetchCount": -1
	},
	"/image/url": {
		"_config": true,
		"description": "외부 이미지 파일",
		"fetch": "https://user-images.githubusercontent.com/23217228/208503656-ea651bc4-5a88-43b7-9405-54cddb780d9c.png"
	},
	"/accounts": {
		"_config": true,
		"description": "응답 데이터가 배열이면 해당하는 목록에서 순차적으로 index에 해당하는 데이터 응답",
		"fetch": ".mock/data/users.json",
		"middlewares": ["_IterateResponse"]
	},
	"/delay": {
		"_config": true,
		"delay": 2000,
		"description": "응답 시간 설정 1000 = 1초",
		"mock": "해당 응답 시간은 2초 입니다."
	},
	"/customStatusCode": {
		"_config": true,
		"statusCode": 500,
		"mock": "해당 응답의 상태코드는 500입니다."
	},
	"/photos": {
		"_config": true,
		"description": "외부 URL 이용",
		"fetch": "http://jsonplaceholder.typicode.com/photos"
	},
	"/posts/:id?": {
		"_config": true,
		"description": "쿼리스트링 사용",
		"fetch": "http://jsonplaceholder.typicode.com${req.url}",
		"fetchCount": -1
	},
	"/posts/customOptions/:id": {
		"_config": true,
		"description": "요청 파라메터 사용.",
		"fetch": {
			"method": "GET",
			"url": "http://jsonplaceholder.typicode.com/posts/${req.params.id}"
		},
		"fetchCount": 5
	},
	"/custom/middlewares": {
		"_config": true,
		"description": "middlewares.js 설정된 커스텀 middleware 사용.",
		"fetch": "http://jsonplaceholder.typicode.com/users",
		"middlewares": ["DataWrapper"]
	}
}
