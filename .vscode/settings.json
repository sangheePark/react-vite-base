{
	// 단어 체크 예외 설정
	"cSpell.words": [
		"vconsole",
		"footerPrefixsSelect",
		"doclock",
		"esbuild",
		"zustand",
		"loglevel",
		"VITE",
		"nprogress",
		"languagedetector",
		"stylelint",
		"commitlint",
		"brotliSize",
		"breakline",
		"issuePrefixs",
		"middlewares",
		"todos",
		"kiran",
		"tanstack",
		"iesc",
		"iescp",
		"docgen"
	],
	// explorer tree tab size
	"workbench.tree.indent": 12,
	// formatter prettier 설정
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	// 저장시 format
	"editor.formatOnSave": true,
	// auto bracketPair
	"editor.bracketPairColorization.enabled": true,
	// auto tag
	"editor.linkedEditing": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports": true,
		"source.fixAll.eslint": true,
		"source.fixAll": true
	},
	"auto-rename-tag.activationOnLanguage": ["*"],
	// mock-server 설정
	"mock-server.settings.port": 3001,
	"mock-server.settings.host": "localhost",
	"mock-server.settings.base": "",
	"mock-server.settings.id": "id",
	"mock-server.settings.dbMode": "multi",
	"mock-server.settings.reverse": false,
	"mock-server.settings.log": false,
	"mock-server.settings.defaults": {
		"noGzip": false,
		"noCors": false,
		"readOnly": false,
		"logger": true,
		"bodyParser": true,
		"cookieParser": true
	},
	"mock-server.settings.paths": {
		"root": "./",
		"db": ".mock/db.json",
		"middlewares": ".mock/middlewares.js",
		"injectors": ".mock/injectors.json",
		"rewriters": ".mock/rewriters.json",
		"static": "public",
		"snapshots": "snapshots",
		"environment": "env"
	},
	"mock-server.settings.watch": true,
	"mock-server.settings.watchFiles": [],
	"mock-server.settings.ignoreFiles": [],
	"mock-server.settings.duplicates": false,
	"mock-server.settings.homePage": true,
	"mock-server.settings.openInside": true,
	"mock-server.settings.showInfoMsg": true,
	"mock-server.settings.statusBar": {
		"show": "true",
		"position": "Right",
		"priority": "0"
	}
}
