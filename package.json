		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-react-remove-attributes": "^1.0.3",
		"vite-plugin-svg-icons": "^2.0.1"
	},
	"browserslist": [
		"last 2 versions"
	],
