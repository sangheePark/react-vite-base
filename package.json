		"react-dom": "^18.2.0",
		"react-router-dom": "^6.6.2",
		"rxjs": "^7.8.0",
		"vconsole": "^3.15.0",
		"zustand": "^4.3.2"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/qs": "^6.9.7",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
