		"eslint-plugin-react-refresh": "^0.3.2",
		"less": "^4.1.3",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"rollup-plugin-visualizer": "^5.9.0",
		"typescript": "^4.9.3",
